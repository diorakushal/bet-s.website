<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="logoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#666666;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#444444;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background circle -->
  <circle cx="256" cy="256" r="240" fill="none" stroke="url(#logoGradient)" stroke-width="2" opacity="0.1"/>
  
  <!-- Abstract geometric icon (left side) -->
  <g transform="translate(120, 156)">
    <!-- Main geometric shape - stylized B or shield -->
    <path d="M0 0 L40 0 L40 80 L0 80 Z" fill="url(#logoGradient)" opacity="0.9"/>
    
    <!-- Internal grid pattern -->
    <rect x="8" y="8" width="24" height="8" fill="none" stroke="url(#logoGradient)" stroke-width="1" opacity="0.6"/>
    <rect x="8" y="24" width="24" height="8" fill="none" stroke="url(#logoGradient)" stroke-width="1" opacity="0.6"/>
    <rect x="8" y="40" width="24" height="8" fill="none" stroke="url(#logoGradient)" stroke-width="1" opacity="0.6"/>
    <rect x="8" y="56" width="24" height="8" fill="none" stroke="url(#logoGradient)" stroke-width="1" opacity="0.6"/>
    
    <!-- Vertical dividers -->
    <line x1="16" y1="8" x2="16" y2="64" stroke="url(#logoGradient)" stroke-width="1" opacity="0.4"/>
    <line x1="24" y1="8" x2="24" y2="64" stroke="url(#logoGradient)" stroke-width="1" opacity="0.4"/>
  </g>
  
  <!-- BETS text (right side) -->
  <g transform="translate(200, 180)">
    <text x="0" y="0" font-family="Inter, Arial, sans-serif" font-size="48" font-weight="800" fill="url(#logoGradient)" letter-spacing="2">
      BETS
    </text>
  </g>
</svg>
